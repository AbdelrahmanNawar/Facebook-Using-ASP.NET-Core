@model IEnumerable<MVCProject.Models.User>
@{
    ViewData["Title"] = "AllUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
    <div class="row p-0 m-0 mt-5">
        <div class="col-10  offset-1 bg-white rounded border-0 p-0 adminUsers pb-5">
            <div class="row p-0 m-0 bg-dark pb-3 rounded-top">
                <div class="input-group md-form form-sm form-1 pl-0 mt-3 col-5 ml-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text bg-light lighten-3" id="basic-text1">
                            <i class="fas fa-search text-primary"
                               aria-hidden="true"></i>
                        </span>
                    </div>
                    <input class="form-control my-0 py-1" id="adminSearchText" type="text" placeholder="Search" aria-label="Search">
                </div>
                <div class="offset-4 col-2 mt-4">
                    <a class="text-primary" asp-controller="Administration" asp-action="CreateUser"><u>Create User</u></a>
                </div>
            </div>
            <div class="row p-0 m-0 d-block" id="adminUsersList">
                <partial name="AdminUserSearch.cshtml"/>
            </div>

        </div>

    </div>




